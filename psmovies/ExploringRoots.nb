Cell[BoxData[
 RowBox[{
  RowBox[{"RootsPlot", "[", 
   RowBox[{"expr_", ",", "z_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"roots", ",", "size", ",", "list"}], "}"}], ",", 
    "\[IndentingNewLine]", " ", 
    RowBox[{
