Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"aloglog", "[", 
   RowBox[{"z_", ",", "n_"}], "]"}], " ", "=", " ", 
  RowBox[{"Sum", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"z", "^", "k"}], " ", "/", " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"Log", "[", 
        RowBox[{"Log", "[", 
         RowBox[{"k", "+", "2"}], "]"}], "]"}], ")"}], "^", "k"}]}], ",", " ",
     
    RowBox[{"{", 
     RowBox[{"k", ",", "0", ",", "n"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.441834874579027*^9, 3.4418349184521136`*^9}, {
  3.4418349533322687`*^9, 3.4418349578287344`*^9}}],

Cell[BoxData[
 RowBox[{
  UnderoverscriptBox["\[Sum]", 
   RowBox[{"k", "=", "0"}], "n"], 
  RowBox[{
   SuperscriptBox["z", "k"], " ", 
   SuperscriptBox[
    RowBox[{"Log", "[", 
     RowBox[{"Log", "[", 
      RowBox[{"2", "+", "k"}], "]"}], "]"}], 
    RowBox[{"-", "k"}]]}]}]], "Output",
 CellChangeTimes->{3.4418350746066527`*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"almostExp", "[", 
   RowBox[{"z_", ",", "n_"}], "]"}], "=", 
  RowBox[{"1", "+", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"z", "^", "k"}], "/", 
      RowBox[{"(", " ", 
       RowBox[{
        RowBox[{"k", "^", "k"}], " ", 
        RowBox[{"Exp", "[", 
         RowBox[{"-", "k"}], "]"}], " ", 
        RowBox[{"Sqrt", "[", 
         RowBox[{"2", " ", "Pi", " ", "k"}], "]"}]}], ")"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"k", ",", "0", ",", "n"}], "}"}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.4418349689447184`*^9, 3.4418350522945695`*^9}}],

Cell[BoxData[
 RowBox[{"1", "+", 
  RowBox[{
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"k", "=", "0"}], "n"], 
   FractionBox[
    RowBox[{
     SuperscriptBox["\[ExponentialE]", "k"], " ", 
     SuperscriptBox["k", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], "-", "k"}]], " ", 
     SuperscriptBox["z", "k"]}], 
    SqrtBox[
     RowBox[{"2", " ", "\[Pi]"}]]]}]}]], "Output",
 CellChangeTimes->{3.441835068377696*^9}]
}, Open  ]],

