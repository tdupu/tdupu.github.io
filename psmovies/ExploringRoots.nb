Cell["\<\
RootsOfPartial outputs a list of complex roots of the nth partial sum of expr \
in terms of complex coordinates\
\>", "Text",
 CellChangeTimes->{{3.44183510027356*^9, 3.441835105731408*^9}, {
  3.441835214958469*^9, 3.4418352496984224`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"RootsOfPartial", "[", 
   RowBox[{"expr_", ",", "z_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"roots", ",", "poly"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"poly", " ", "=", " ", 
      RowBox[{"Normal", "[", 
       RowBox[{"Series", "[", 
        RowBox[{"expr", ",", 
         RowBox[{"{", 
          RowBox[{"z", ",", "0", ",", "n"}], "}"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"roots", " ", "=", " ", 
      RowBox[{"z", "/.", " ", 
       RowBox[{"NSolve", "[", 
        RowBox[{
         RowBox[{"poly", "\[Equal]", "0"}], ",", "z"}], "]"}]}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.441830108245376*^9, 3.4418302540049677`*^9}, {
   3.441830284488802*^9, 3.4418302864916816`*^9}, {3.4418304287161903`*^9, 
   3.441830442295717*^9}, 3.44183509651816*^9}],

Cell["\<\
ListRootsOfPartial gives the Roots of the nth partial sum of expr in list \
form, where 1st coordinate is the real part and the 2nd coordinate is the \
Imaginary coordinate \
\>", "Text",
 CellChangeTimes->{{3.441835122996234*^9, 3.4418352088797283`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"ListRootsOfPartial", "[", 
   RowBox[{"expr_", ",", "z_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "roots", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"roots", " ", "=", " ", 
      RowBox[{"RootsOfPartial", "[", 
       RowBox[{"expr", ",", "z", ",", "n"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"roots", " ", "=", " ", 
      RowBox[{"Transpose", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Re", "[", "roots", "]"}], ",", 
         RowBox[{"Im", "[", "roots", "]"}]}], "}"}], "]"}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.441830262497179*^9, 3.441830404972048*^9}, {
   3.441830444398741*^9, 3.441830490474995*^9}, {3.441830601134115*^9, 
   3.4418306046191263`*^9}, 3.4418351181692924`*^9, 3.441835152228267*^9}],

